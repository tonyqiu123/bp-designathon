    "extra": {
      "apiUrl": "https://api.wat2do.ca/api"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wat2do.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wat2do.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
